{
  "name": "spotify-player",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/spotify-player",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/spotify-player",
        "outputPath": "dist/apps/spotify-player"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/spotify-player"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "spotify-player:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "spotify-player:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "spotify-player:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "spotify-player:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/spotify-player/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/spotify-player/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "codegen": {
      "command": "graphql-codegen --require dotenv/config --config apps/spotify-player/codegen.ts"
    },
    "component-test": {
      "command": "cypress open --project apps/spotify-player --component --browser chrome"
    }
  },
  "tags": []
}
